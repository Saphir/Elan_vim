int main(int argc, char * argv[]) {
#ifndef NDEBUG
    std::cout << "[MAIN] argc: " << argc << std::endl;
    std::cout << "[MAIN] argv: ";
    for (std::int32_t i {0}; i < argc; ++i) {
        std::cout << argv[i];
        if (i == argc - 1) {
            std::cout << std::endl;
        } else {
            std::cout << " ";
        }
    }
#endif
    std::endl(std::cout << "Hello World!");

    // exit(EXIT_FAILURE);
    return EXIT_SUCCESS;
}
