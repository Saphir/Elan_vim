# random int
import "math/rand"
rand.Seed(time.Now().UnixNano())
i := rand.Intn(100) // [0, 100)

# wait for goroutine
import "sync"
wg := sync.WaitGroup
wg.Add(3)
defer wg.Done() # in goroutine function
wg.Wait()

# string, int, hex, bin convert
strconv.Atoi(<string>)
strconv.Itoa(<int>)
strconv.ParseInt("0xff", 0, 32)
strconv.ParseInt("0b101101", 0, 32)
fmt.Sprintf("0x%x", 255)
fmt.Sprintf("0b%b", 23)
